<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <!-- Объявляем имя сервлета и его класс,
    а также присваиваем ему url паттерн-->
    <servlet>
        <servlet-name>main</servlet-name>
        <servlet-class>com.elina.rbp107.MyServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>main</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>diagram</servlet-name>
        <servlet-class>com.elina.rbp107.MyDiagram</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>diagram</servlet-name>
        <url-pattern>/diagram</url-pattern>
    </servlet-mapping>

</web-app>